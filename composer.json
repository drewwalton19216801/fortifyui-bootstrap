{
    "name": "drewwalton19216801/fortifyui-bootstrap",
    "description": "Fortify-driven Laravel UI replacement using Bootstrap 5",
    "keywords": [
        "fortify-ui"
    ],
    "homepage": "https://github.com/drewwalton19216801/fortifyui-bootstrap",
    "license": "MIT",
    "authors": [
        {
            "name": "Drew Walton",
            "email": "drewwalton19216801@gmail.com",
            "homepage": "https://github.com/drewwalton19216801/fortifyui-bootstrap",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/contracts": "^8.0",
        "zacksmash/fortify-ui": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "drewwalton19216801\\FortifyUIBootstrap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "drewwalton19216801\\FortifyUIBootstrap\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
              "FortifyUIBootstrapServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
